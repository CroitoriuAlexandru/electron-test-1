{
  "name": "aipro",
  "version": "0.0.1",
  "icon": "icon.ico",
  "description": "Aipro Browser",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --config webpack.common.js --watch",
    "start": "electron .",
    "tailwind": "npx tailwindcss -i ./tailwind.css -o ./pages/tailwind.css --watch"
  },
  "keywords": [],
  "author": "Aipro",
  "license": "ISC",
  "dependencies": {
    "@balkangraph/orgchart.js": "^8.14.10",
    "flowbite": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "sqlite3": "^5.1.7",
    "sequelize": "^6.37.1"
  },
  "build": {
    "appId": "com.aipro.browser",
    "productName": "Aipro",
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "electron": "^29.1.1",    
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.35",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
